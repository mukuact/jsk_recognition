<launch>
  <node name="image_publisher"
        pkg="jsk_perception" type="image_publisher.py">
    <rosparam subst_value="true">
      file_name: $(find jsk_perception)/sample/object_detection_example_1.jpg
      encoding: bgr8
      publish_info: false
    </rosparam>
  </node>

  <node name="fast_rcnn"
        pkg="jsk_perception" type="faster_rcnn_caffenet.py" output="screen">
    <remap from="~input" to="/image_publisher/output" />
    <rosparam>
      model: vgg_cnn_m_1024
    </rosparam>
  </node>

  <node name="draw_rect_array"
        pkg="jsk_perception" type="draw_rect_array.py">
    <remap from="~input" to="image_publisher/output" />
    <remap from="~input/rect_array" to="fast_rcnn/rect_array" />
  </node>

  <node name="image_view"
        pkg="image_view" type="image_view">
    <remap from="image" to="/draw_rect_array/output" />
  </node>

</launch>
